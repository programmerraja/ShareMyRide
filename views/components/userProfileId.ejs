<section>
	<div class="user_profile">
		<!-- user profile top -->
		<div class="user_profile-top">
			<div class="user_profile-img">
				 <img src="/public/img/user.svg" class="rider_img">
			</div>
			<div class="user_profile-name">
				<p><%= user_profile.name %></p>
			</div>
		</div>

		<!-- user profile bottom -->
		<div class="user_profile-bottom">
			<div class="user_profile-text">
				<p class="text-muted">"<%= user_profile.bio %> hai "</p>
			</div>
			
			<div class="user_profile-text">
				<p class="text-capitalize "><i class="fab fa-whatsapp"></i>WhatsApp no:</p>
				<p class="ml-5"><%= user_profile.whatsappno %></p>

				
			</div>
			<div class="user_profile-text">
				<p class="text-capitalize "><i class="fas fa-phone-volume"></i>Phone no:</p>
				<p><%= user_profile.phoneno %></p>

				
			</div>
			<div class="user_profile-text">
				<p class="text-capitalize "><i class="fas fa-car"></i>Driving expereince:</p>
				<p><%= user_profile.drivingexpereince %></p>

				
			</div>
		
		</div>

	</div>
</section>