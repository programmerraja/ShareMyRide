<section>
	<div class="user_profile">
		<!-- user profile top -->
		<div class="user_profile-top">
			<div class="user_profile-img">
				 <img src="/public/img/user.svg" class="user_img">
			</div>
			<div class="user_profile-name">
				<p><%= user_profile.name %></p>
			</div>
		</div>

		<!-- user profile bottom -->
		<div class="user_profile-bottom">
			<div class="user_profile-bio">
				<p>Bio:</p>
				<p><%= user_profile.bio %></p>
				
			</div>
			
			<div class="user_profile-whatsapp">

				<p><%= user_profile.whatsappno %></p>

				
			</div>
			<div class="user_profile-phoneno">
				<p><%= user_profile.phoneno %></p>

				
			</div>
			<div class="user_profile-experience">
				<p>Driving Experince</p>
				<p><%= user_profile.drivingexpereince %></p>

				
			</div>
		
		</div>

	</div>
</section>